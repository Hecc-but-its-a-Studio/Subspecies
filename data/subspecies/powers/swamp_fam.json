          },
          {
             "type":"origins:execute_command",
             "command":"team join swamp_sub @e[type=minecraft:witch]"
          },
          {
            "type":"origins:execute_command",
            "command":"team join swamp_sub @e[type=minecraft:slime]"
         }
       ]
    },
    "interval":20
}